{% extends 'bookings.html.twig' %}
{% block content %}
    <h2>The List of Bookings</h2>
    {% if bookings is empty %}
    <p>You do not have any bookings</p>
    {% else %}
        <table>
            <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Phone</th>
                <th>Email</th>
                <th>Birthday</th>
                <th>Start Date</th>
                <th>End Date</th>
                <th>Arrival Time</th>
                <th>Number of people</th>
                <th>Payment</th>
            </tr>
            </thead>
        </table>
    {% endif %}
{% endblock %}